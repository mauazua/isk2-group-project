        <Label x:Name="label_Copy2" Content="Miasto" HorizontalAlignment="Left" Height="28" Margin="10,136,0,0" VerticalAlignment="Top" Width="90"/>
        <Label x:Name="label_Copy3" Content="Kraj" HorizontalAlignment="Left" Height="28" Margin="10,182,0,0" VerticalAlignment="Top" Width="90" RenderTransformOrigin="0.613,2.027"/>
        <Label x:Name="label_Copy4" Content="Cena" Height="28" Margin="10,231,5.6,0" VerticalAlignment="Top" RenderTransformOrigin="0.613,2.027"/>
