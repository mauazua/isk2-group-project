<Window x:Class="Admin.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Admin"
        mc:Ignorable="d"
        Title="Window3" Height="300" Width="300"
        Icon="palmTree.ico">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="125,56,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="125,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="User_ID" HorizontalAlignment="Left" Margin="23,53,0,0" VerticalAlignment="Top" Width="60"/>
        <Label x:Name="label1" Content="Event_ID" HorizontalAlignment="Left" Margin="23,135,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.166,0.444"/>
        <Button x:Name="button" Content="Usuń" HorizontalAlignment="Left" Margin="170,215,0,0" VerticalAlignment="Top" Width="75" Click="usuń"/>
        <Button x:Name="button1" Content="Cofnij" HorizontalAlignment="Left" Margin="23,215,0,0" VerticalAlignment="Top" Width="75" Click="cofnij"/>

    </Grid>
</Window>
