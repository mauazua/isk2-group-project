        mc:Ignorable="d"
        Title="Dodaj Wycieczkę" Height="424.85" Width="409.6"
        Icon="palmTree.ico">
    <Grid Margin="0,0,3.2,2.2" RenderTransformOrigin="0.561,0.499" Height="393" VerticalAlignment="Bottom">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="113.6"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Nazwa" HorizontalAlignment="Left" Height="26" Margin="10,10,0,0" VerticalAlignment="Top" Width="90"/>
        <Label x:Name="label_Copy" Content="Data Rozpoczęcia" HorizontalAlignment="Left" Height="29" Margin="10,52,0,0" VerticalAlignment="Top" Width="108" RenderTransformOrigin="0.453,1.445" Grid.ColumnSpan="2"/>
